function ValueFormatters(){}

ValueFormatters.formatFixedValueOrNull = function( valueOrNull, precision ) {
    precision = precision || 2;
    if( valueOrNull != null) {
        return valueOrNull.toFixed( precision );
    }
    return 0.0;
};